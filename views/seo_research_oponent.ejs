    <script type="text/javascript">
        $(document).ready(function(){
            $('#scan').click(function(){
                var url = $('input#url').val();
                $.ajax({
                    url: '/post/seo-research-oponent',
                    type: 'post',
                    data: {
                      url: url
                    },
                    success: function(dt){
                        swal({
                          title:"Hoàn Tất!",
                          type:"success",
                          timer: 1000,   
                          showConfirmButton: false
                           }
                          );
                        $.each(dt, function(key, value) {
                              if(value.value == undefined || value.value == ""){
                                  value.value = "Chưa Có";
                              }
                              $('tbody#result').append("<tr><td class='col s3 m3 l3'>"+value.name+"</td><td class='col s9 m9 l9'>"+value.value+"</td></tr>");
                        });
                    }
                });
            });
        });
    </script>

