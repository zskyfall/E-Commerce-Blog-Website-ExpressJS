    <script type="text/javascript">
        
        $(document).ready(function(){
              $('a.edit').click(function(){
                  var id = $(this).attr('id');

                  $.ajax({
                      url: '/category/info',
                      type: 'post',
                      data: {
                        id: id
                      },
                      success: function(dt){
                          $('input[name="edit_id"]').val(dt._id);
                          $('input[name="edit_code"]').val(dt.code);
                          $('input[name="edit_name"]').val(dt.name);
                          $('input[name="edit_permalink"]').val(dt.permalink);
                          $('select[name="edit_parent"]').val(dt.parent);
                          $('input[name="edit_description"]').val(dt.description);
                          
                      }
                  });
              }); 

              $('a.remove').click(function(){
                  var id = $(this).attr('id');
                  var par = $(this).parent().parent();
                  $.ajax({
                      url: '/category/remove',
                      type: 'post',
                      data: {
                        id: id
                      },
                      success: function(dt){
                          if(dt == "ok"){
                            par.fadeOut();
                            swal("Thành Công", "", "success");
                            
                          }
                          else{
                            swal("Thất Bại", "Xảy ra lỗi! Thử lại?", "error");
                          }

                      }
                  });
              });

        });

    </script>

    

