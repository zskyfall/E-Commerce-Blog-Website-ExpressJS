        <div class="container">

          <div class="row">
                
                  <div id="modal2" class="modal">
                  <div class="modal-content">
                          
                          <div class="card-panel">
                              <h4 class="header2">Sửa Danh Mục</h4>
                              <div class="row">
                                <form class="col s12" action="/admin/category/edit" method="post">
                                  <div class="row">
                                    <div class="input-field col s12">
                                      <i class="mdi-content-gesture prefix active"></i>
                                      <input id="id" type="hidden" name="edit_id">
                                      <input id="code" type="text" name="edit_code" placeholder="VD: MB">
                                      <label  class="active" for="code">Mã Danh Mục</label>
                                    </div>


                                  </div>
                                  <div class="row">
                                    <div class="input-field col s12">
                                      <i class="mdi-content-content-paste prefix"></i>
                                      <input id="name" type="text" name="edit_name" placeholder="VD: Điện Thoại">
                                      <label for="name">Tên Danh Mục</label>
                                    </div>
                                  </div>

                                  <div class="row">
                                    <div class="input-field col s12">
                                      <i class="mdi-social-public prefix"></i>
                                      <input id="permalink" type="text" name="edit_permalink" placeholder="VD: mobile">
                                      <label for="permalink">Chuỗi cho đường dẫn tĩnh</label>
                                    </div>
                                  </div>
                                 
                                 <div class="row">
                                        <div class="col s12 m8 l9">
                                          <label>Danh Mục Cha</label>
                                          <select class="browser-default" name="edit_parent">
                                            <option value="">Chọn</option>
                                         
                                            
                                          </select>
                                        </div>
                                 </div>

                                <div class="row">
                                    <div class="input-field col s12">
                                      <i class="mdi-editor-mode-edit prefix"></i>
                                      <input id="description" type="text" name="edit_description" placeholder="VD: Điện thoại di động...">
                                      <label for="description">Mô Tả</label>
                                    </div>
                                  </div>
                                  <div class="row">

                                    <div class="row">
                                      <div class="input-field col s12">
                                        <button class="btn cyan waves-effect waves-light right" type="submit">Hoàn Tất
                                          <i class="mdi-content-send right"></i>
                                        </button>
                                      </div>
                                    </div>
                                  </div>
                                </form>
                              </div>
                </div>

                  </div>
                  <div class="modal-footer">
                    <a href="#" class="waves-effect waves-red btn-flat modal-action modal-close">Đóng</a>
                    
                  </div>
                </div>



              </div>
            </div>

          <div class="row">
                <div class="col s12 m12 l12">
                  <table class="striped">
                    <thead>
                      <tr>
                        <th data-field="id">Thời Gian</th>
                        <th data-field="name">Họ Tên</th>
                        <th data-field="permalink">Số Điện Thoại</th>
                        <th data-field="parent">Địa Chỉ</th>
                        <th data-field="description">Sản Phẩm</th>
                        <th data-field="description">Số Lượng</th>
                        <th data-field="edit">Nhận</th>
                        <th data-field="remove">Xóa</th>
                      </tr>
                    </thead>
                    <tbody>
                      
                        <% order.forEach(function(c){%>
                      <tr>
                        <td><%= c.created_at %> </td>
                        <td> 
                            <% if(c.customer_type == "guest"){%>
                              <%= c.customer_name %>
                            <%}%>
                            <% if(c.customer_type != "guest"){%>
                              <a href="/admin/user?id=<%= c.customer_id %>"><%= c.customer_name %></a>
                            <%}%>
                        </td>
                        <td><%= c.customer_phone %></td>
                        <td><%= c.customer_address %> </td>
                        <td><%= c.product_id %> </td>
                        <td><a class="btn-floating waves-effect waves-light cyan edit btn modal-trigger" id="<%= c._id %>" href="#modal2"><i class="mdi-editor-mode-edit"></i></a></td>
                        <td><a class="btn-floating waves-effect waves-light red remove" id="<%= c._id %>"><i class="mdi-content-clear"></i></a></td>
                      </tr>
                        <%});%>
                        
                        
                      
            
                    </tbody>
                  </table>
                </div>
          </div>

          <div id="dialog2" title="Image Full Size View">
              <img id="imagePreview" />
          </div>
          
          
